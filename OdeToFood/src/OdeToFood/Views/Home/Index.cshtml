@model OdeToFood.ViewModels.HomePageVM

<html>
<head>
    <title>OdeToFood</title>
</head>

<body>
    <h1>Restaurant list</h1>
    <h3>@Model.CurrentMessage</h3>
    <table>

        @foreach (var restaurant in Model.Restaurants)
        {
            <tr>
                <td>@Html.ActionLink("View", "Detail", new { id = restaurant.Id })</td>
                <td>@restaurant.Name</td>
            </tr>
        }
    </table>
</body>
</html>